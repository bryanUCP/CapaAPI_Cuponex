<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="promocion_sucursal">
    
    <insert id="registrar" parameterType="pojos.Promocion_Sucursal">
        INSERT INTO promociones_sucursales(idPromocion, idSucursal) 
        VALUES (#{idPromocion},#{idSucursal})
    </insert>
    
</mapper>

